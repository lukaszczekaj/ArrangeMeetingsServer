<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Company" table="company">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="pass" type="text" column="pass" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="addressstreet" type="string" column="addressStreet" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="addresscity" type="string" column="addressCity" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="addresspostalcode" type="string" column="addressPostalCode" length="6" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
